<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.nfc.connect.nfc.Truck.CheckDataActivity"
    android:orientation="vertical">
    <TextView

        android:gravity="right"
        android:textSize="20dp"
        android:layout_marginRight="10dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="15dp"
        android:layout_width="match_parent"
        android:id="@+id/check_data_farm_id"
        android:layout_height="wrap_content" />
    <TextView

        android:gravity="right"
        android:textSize="20dp"
        android:layout_marginRight="10dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="15dp"
        android:layout_width="match_parent"

        android:id="@+id/check_data_truck_id"
        android:layout_height="wrap_content" />
    <TextView

        android:gravity="right"
        android:textSize="20dp"
        android:layout_marginRight="10dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="15dp"
        android:layout_width="match_parent"
        android:id="@+id/check_data_retailer_id"
        android:layout_height="wrap_content" />
    <TextView

        android:gravity="right"
        android:textSize="20dp"
        android:layout_marginRight="10dp"
        android:layout_marginLeft="30dp"

        android:layout_gravity="left"
        android:layout_marginTop="15dp"
        android:layout_width="match_parent"
        android:id="@+id/check_data_type"
        android:layout_height="wrap_content" />
    <TextView

        android:gravity="right"
        android:textSize="20dp"
        android:layout_marginRight="10dp"
        android:layout_marginLeft="30dp"

        android:layout_gravity="left"
        android:layout_marginTop="16dp"
        android:layout_width="match_parent"
        android:id="@+id/check_data_clasificarion"
        android:layout_height="wrap_content" />
    <TextView

        android:gravity="right"
        android:textSize="20dp"
        android:layout_marginRight="10dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="15dp"
        android:layout_width="match_parent"
        android:id="@+id/check_data_farm_location"
        android:layout_height="wrap_content" />
    <TextView

        android:gravity="right"
        android:textSize="20dp"
        android:layout_marginRight="10dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="15dp"
        android:layout_width="match_parent"
        android:id="@+id/check_data_retailer_location"
        android:layout_height="wrap_content" />
    <TextView

        android:gravity="right"

        android:textSize="20dp"
        android:layout_marginRight="10dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="15dp"
        android:layout_width="match_parent"
        android:id="@+id/check_data_farm_date"
        android:layout_height="wrap_content" />
    <TextView

        android:gravity="right"
        android:textSize="20dp"
        android:layout_marginRight="10dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="15dp"
        android:layout_width="match_parent"
        android:id="@+id/check_data_retailer_date"
        android:layout_height="wrap_content" />

</LinearLayout>